{
  "manifest_version": 2,
  "name": "Highlighted Translate",
  "version": "0.0.1",
  "description": "Extension for translate text selected",
  "web_accessible_resources": ["images/save-icon.svg", "images/list.svg", "images/trash.svg"],
  "page_action": {
    "default_popup": "",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png"
    }
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["css/content.css"]
    }
  ]
}
