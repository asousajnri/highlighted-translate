{
  "manifest_version": 2,
  "name": "Highlighted Translate",
  "version": "0.0.1",
  "description": "Extension for translate text selected",
  "page_action": {
    "default_popup": "",
    "default_icon": {
      "19": "icons/19x19.png",
      "38": "icons/38x38.png"
    }
  },
  "icons": {
    "19": "icons/19x19.png",
    "38": "icons/38x38.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["./content/popup-translate.css"],
      "js": ["./content/content.min.js"]
    }
  ]
}
