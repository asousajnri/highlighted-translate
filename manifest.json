{
  "manifest_version": 2,
  "name": "Highlighted Translate",
  "version": "0.0.1",
  "description": "Extension for translate text selected",
  "page_action": {
    "default_popup": "",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png"
    }
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
